bench_aggregate_autovec_smallint_VL128_to_512: benchmark.a
	icpx \
	-fsycl $(ALL_CXXFLAGS) -v \
	$(foreach L,$(INC_DIRS),-I$L) \
	$(foreach D,$(DEFINES),-D$D) \
	-DUI8 -DUI16 -DVL128 -DVL256 -DVL512 \
	-fintelfpga -Xshardware -fsycl-device-code-split=off \
	-Xssave-temps \
	-reuse-exe=build/$@.fpga -Xsoutput-report-folder=build/$@.prj \
	-Xsboard=$(BOARD) \
	-qactypes -fsycl-unnamed-lambda \
	-o build/$@.fpga \
	src/intel_simd/filtered_agg/aggregate_autovec_bench_oneApi.cpp $(foreach D,$(LIB_DIRS),-L$D) $(foreach L,$(LIBS),-l$L) -static

bench_aggregate_autovec_smallint_VL1K_to_4K: benchmark.a
	icpx \
