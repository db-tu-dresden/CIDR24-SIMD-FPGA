	-fintelfpga -Xshardware -fsycl-device-code-split=off \
	-Xssave-temps \
	-reuse-exe=build/$@.fpga -Xsoutput-report-folder=build/$@.prj \
	-Xsboard=$(BOARD) \
	-qactypes -fsycl-unnamed-lambda \
	-o build/$@.fpga \
	src/intel_simd/filtered_agg/aggregate_autovec_bench_oneApi.cpp $(foreach D,$(LIB_DIRS),-L$D) $(foreach L,$(LIBS),-l$L) -static

