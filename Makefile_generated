	-Xsboard=$(BOARD) \
	-qactypes -fsycl-unnamed-lambda \
	-o build/$@.fpga \
	src/lzc_unrolled_and_aggregate_test.cpp $(foreach D,$(LIB_DIRS),-L$D) $(foreach L,$(LIBS),-l$L) -static

bench_lzc_unrolled_and_aggregate_outDevice_bigint_VL4k: benchmark.a
	icpx \
	-fsycl $(ALL_CXXFLAGS) -v \
	$(foreach L,$(INC_DIRS),-I$L) \
	$(foreach D,$(DEFINES),-D$D) \
